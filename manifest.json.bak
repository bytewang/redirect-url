{
   "name": "checkPage",
   "description": "check page on my page-analyser",
   "version":"1.0",
   "manifest_version": 2,
   "permissions": [ "tabs", "http://*/", "https://*/" ],
   "browser_action": {
      "default_icon": "icon.gif",
      "default_popup": "popup.html"
   },
   "options_page": "options.html",
   "background": {
    "scripts": ["background.js"],
    "persistent": false
  }	
}
